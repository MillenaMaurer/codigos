      "source": [
        "n = int(input(\"Digite um numero: \"))\n",
        "fatorial = 1\n",
        "M = 1\n",
        "if n == 0:\n",
        "  print(\"Fatorial de 0 = 1\")\n",
        "  while M <= n :\n",
        "    fatorial = fatorial * M\n",
        "    M = M + 1\n",
        "print(f\" fatorial do {n}! = {fatorial}\")"
      ]
    }
