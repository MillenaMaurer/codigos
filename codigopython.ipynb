      "source": [
        "salariox = float(input(\"Digite o salário inicial: \"))\n",
        "anoinicial = 1995\n",
        "salarioy = salariox\n",
        "atual_ano = anoinicial + 1\n",
        "percentual = 0.015\n",
        "while atual_ano <= 2024:\n",
        "    percentual *= 2\n",
        "    salarioy += salarioy * percentual\n",
        "    atual_ano += 1\n",
        "print(f\"O salário atual é R${salarioy}\")"
      ]
    }
  ]
